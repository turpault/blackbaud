{
  "name": "blackbaud-oauth-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/blackbaud",
  "description": "A modern React application that implements OAuth 2.0 authentication with Blackbaud's API services.",
  "dependencies": {
    "axios": "^1.3.4",
    "dexie": "^4.0.11",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "nodemon": "^3.1.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:branch": "node scripts/build-branch.js",
    "build:dev": "GENERATE_SOURCEMAP=true react-scripts build",
    "dev:watch": "nodemon --watch src --ext ts,tsx,js,jsx,json --exec \"GENERATE_SOURCEMAP=true npm run build\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ts-run": "ts-node",
    "tsx-run": "tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "react",
    "oauth",
    "blackbaud",
    "authentication",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
